                                          isco08 == 2000 ~ 2000,
                                          isco08 == 2100 ~ 2100,
                                          isco08 == 2110 ~ 2110,
                                          isco08 == 2111 ~ 2111,
                                          isco08 == 2112 ~ 2112,
                                          isco08 == 2113 ~ 2113,
                                          isco08 == 2114 ~ 2114,
                                          isco08 == 2120 ~ 2120,
                                          isco08 == 2130 ~ 2210,
                                          isco08 == 2131 ~ 2211,
